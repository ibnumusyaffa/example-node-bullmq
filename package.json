{
  "name": "starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node ./app/server.js",
    "dev": "nodemon ./app/server.js",
    "lint": "eslint --ignore-path .gitignore  --ext .js,.ts,.tsx .",
    "lint:fix": "eslint --ignore-path .gitignore  --ext .js,.ts,.tsx --fix .",
    "format": "pretty-quick --ignore-path .gitignore --pattern \"**/*.+(js|jsx|tsx)\"",
    "format:all": "prettier --ignore-path .gitignore --write \"**/*.+(js|jsx|tsx)\"",
    "format:check": "prettier --ignore-path .gitignore --check \"**/*.+(js|jsx|tsx)\"",
    "validate": "npm run lint && npm run format && npm run test",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migration:generate": "npx sequelize migration:generate",
    "db:model:generate": "npx sequelize model:generate",
    "db:seed:generate": "npx sequelize seed:generate",
    "db:migrate:undo": "npx sequelize db:migrate:undo",
    "db:migrate:undo:all": "npx sequelize db:migrate:undo:all",
    "db:seed": "npx sequelize db:seed",
    "db:seed:all": "npx sequelize db:seed:all",
    "db:reset": "npx sequelize db:drop && npx sequelize db:create && npx sequelize-cli db:migrate"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bullmq": "^3.10.0",
    "compression": "^1.7.4",
    "consola": "^2.15.3",
    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "mysql2": "^3.1.2",
    "npm": "^9.5.1",
    "pino": "^8.11.0",
    "pino-http": "^8.3.3",
    "sequelize": "^6.29.0"
  },
  "devDependencies": {
    "@jackfranklin/test-data-bot": "^2.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "sequelize-cli": "^6.6.0",
    "supertest": "^6.3.3"
  }
}
